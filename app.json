{
  "name": "HackPSU",
  "displayName": "HackPSU",
  "expo": {
    "name": "HackPSU",
    "version": "0.9.2",
    "icon": "./assets/images/logo.png",
    "slug": "hackpsu",
    "privacy": "unlisted",
    "entryPoint": "index.js",
    "primaryColor": "#6A85B9",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "assets/images/*"
    ],
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#FFFFFF"
    },
    "android": {
      "package": "org.hackpsu.HackPSU",
      "versionCode": 9,
      "useNextNotificationsApi": true
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#6A85B9"
    },
    "ios": {
      "bundleIdentifier": "org.hackpsu.HackPSU",
      "buildNumber": "0.9.2",
      "usesAppleSignIn": true
    },
    "extra": {
      "apiBaseUrl.staging": "https://staging-dot-hackpsu18.appspot.com/v1/",
      "apiBaseUrl.prod": "https://api.hackpsu.org/v1/",
      "apiBaseUrlV2.staging": "http://staging.hackpsu18.appspot.com/v2/",
      "apiBaseUrlV2.prod": "https://api.hackpsu.org/v2/"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg"
      ]
    }
  }
}
